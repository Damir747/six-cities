    "react-dom": "16.14",
    "react-redux": "7.2",
    "react-router-dom": "5.2",
    "redux": "4.2.1",
    "redux-devtools-extension": "2.13",
    "redux-thunk": "2.3",
    "reselect": "^3.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.12",
    "@babel/preset-env": "7.12",
    "@babel/preset-react": "7.12",
